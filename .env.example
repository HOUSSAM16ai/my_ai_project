# إعدادات تطبيق Flask
FLASK_DEBUG=1
SECRET_KEY=please_change_me_to_a_very_long_and_random_secret_key

# إعدادات قاعدة البيانات PostgreSQL
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=mydb